    <!-- BEGIN: Hybrid part of the GDAS workflow -->

    <!-- BEGIN: GDAS EOBS -->
    <task name="eobs" cycledefs="gdas" maxtries="&MAXTRIES;">

        <command>&JOBS_DIR;/eobs.sh</command>

        <jobname><cyclestr>&PSLOT;_eobs_@H</cyclestr></jobname>
        <account>&ACCOUNT;</account>
        <queue>&QUEUE_EOBS;</queue>
        &RESOURCES_EOBS;
        <walltime>&WALLTIME_EOBS;</walltime>
        <native>&NATIVE_EOBS;</native>

        <join><cyclestr>&ROTDIR;/logs/@Y@m@d@H/eobs.log</cyclestr></join>

        &eEXPDIR;
        &eCDATE;
        &eCDUMPGDAS;

        <dependency>
            <and>
                <taskdep task="prep"/>
                <taskdep task="epos" cycle_offset="-06:00:00"/>
            </and>
        </dependency>

    </task>
    <!-- END: GDAS EOBS -->

    <!-- BEGIN: GDAS EOMN -->
    <metatask name="eomn">

        <var name="grp">@EOMGGROUPS@</var>

        <task name="eomg#grp#" cycledefs="gdas" maxtries="&MAXTRIES;">

            <command>&JOBS_DIR;/eomg.sh</command>

            <jobname><cyclestr>&PSLOT;_eomg#grp#_@H</cyclestr></jobname>
            <account>&ACCOUNT;</account>
            <queue>&QUEUE_EOMG;</queue>
            &RESOURCES_EOMG;
            <walltime>&WALLTIME_EOMG;</walltime>
            <native>&NATIVE_EOMG;</native>

            <join><cyclestr>&ROTDIR;/logs/@Y@m@d@H/eomg#grp#.log</cyclestr></join>

            &eEXPDIR;
            &eCDATE;
            &eCDUMPGDAS;

            <envar>
                <name>ENSGRP</name>
                <value>#grp#</value>
            </envar>

            <dependency>
                <taskdep task="eobs"/>
            </dependency>

        </task>

    </metatask>
    <!-- END: GDAS EOMN -->

    <!-- BEGIN: GDAS EUPD -->
