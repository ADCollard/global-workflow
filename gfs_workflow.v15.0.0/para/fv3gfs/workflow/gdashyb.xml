    <!-- END: GDAS ECEN -->

    <!-- BEGIN: GDAS EFMN -->
    <metatask name="efmn">

        <var name="grp">@EFCSGROUPS@</var>

        <task name="efcs#grp#" cycledefs="gdas" maxtries="&MAXTRIES;">

