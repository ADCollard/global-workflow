            &eCDUMPGDAS;

            <envar>
                <name>ENSGRP</name>
                <value>#grp#</value>
            </envar>

            <dependency>
                <or>
                    <taskdep task="ecen"/>
                    <not>
                        <cycleexistdep cycle_offset="-6:00:00"/>
                    </not>
                </or>
            </dependency>

        </task>

    </metatask>
    <!-- END: GDAS EFMN -->

    <!-- BEGIN: GDAS EPOS -->
