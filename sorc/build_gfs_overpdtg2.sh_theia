#! /bin/sh

######################################################################
#
# Build executable utility: overpdtg2 using module compile standard
#
######################################################################
######################################################################
set -x
mod=$( cd ../modulefiles/ ; pwd -P )
source "$mod/module-setup.sh.inc"
module use ../modulefiles
module load gfs_overpdtg2.theia

cd overpdtg2.fd
make
make clean
mv overpdtg2   ../../exec/
