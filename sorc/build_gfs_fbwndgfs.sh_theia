#!/bin/sh
set -x -e

######################################################################
#
# Build executable utility: fbwndgfs using module compile standard
#
######################################################################
######################################################################

mod=$( cd ../modulefiles/ ; pwd -P )
source "$mod/module-setup.sh.inc"
module use ../modulefiles
module load gfs_fbwndgfs.theia

cd fbwndgfs.fd
make
make clean
mv fbwndgfs   ../../exec/
