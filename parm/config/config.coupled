#!/bin/ksh -x

########## config.coupled ##########
# Common to all steps

# for uncoupled workflow, set cpl to false
#export cpl=".false."

# for coupled workflow, set cpl to true
export cpl=".true."
if [ $cpl = ".false." ]; then
export cplflx=".F."
echo "END: run uncoupled fv3gfs model"
else
# for coupled workflow
export CPLFLX=".T."
export CPLICE=".T."
export cpl=".true."
export print_esmf=".false."

export confignamevarfornems='med_atm_ocn_ice'
echo "END: config.coupled"
